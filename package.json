{
	"name": "sort-json",
	"displayName": "Sort JSON",
	"description": "Simple JSON Object and Array sort",
	"version": "9.1.0",
	"publisher": "Thinker",
	"license": "MIT",
	"icon": "images/sort-json-logo.png",
	"author": {
		"name": "Sivaraman",
		"email": "sendmsg2siva@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/R35007/sort-json-vscode-extension"
	},
	"keywords": [
		"JSON",
		"Sort JSON",
		"sort",
		"order json",
		"json sort",
		"object sort",
		"sorting",
		"array sort",
		"sort array",
		"array",
		"object",
		"order"
	],
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sort-json.sortJSON",
		"onCommand:sort-json.sortJSONDeep",
		"onCommand:sort-json.sortJSONReverse",
		"onCommand:sort-json.sortJSONDeepReverse",
		"onCommand:sort-json.sortJSONByCustomComparison",
		"onCommand:sort-json.setCaseSensitive",
		"onCommand:sort-json.setCaseInSensitive",
		"onCommand:sort-json.setSortType"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sort-json.sortJSON",
				"title": "Sort",
				"category": "Sort JSON"
			},
			{
				"command": "sort-json.sortJSONDeep",
				"title": "Deep Sort",
				"category": "Sort JSON"
			},
			{
				"command": "sort-json.sortJSONReverse",
				"title": "Reverse Sort ",
				"category": "Sort JSON"
			},
			{
				"command": "sort-json.sortJSONDeepReverse",
				"title": "Deep Reverse Sort",
				"category": "Sort JSON"
			},
			{
				"command": "sort-json.sortJSONByCustomComparison",
				"title": "Custom Comparison Sort",
				"category": "Sort JSON"
			},
			{
				"command": "sort-json.setSortType",
				"title": "Set Sort Type",
				"category": "Sort JSON"
			},
			{
				"command": "sort-json.toggleCaseSensitive",
				"title": "Toggle Case-Sensitive",
				"category": "Sort JSON"
			},
			{
				"command": "sort-json.setCaseSensitive",
				"title": "Is Case-Sensitive",
				"category": "Sort JSON"
			},
			{
				"command": "sort-json.setCaseInSensitive",
				"title": "✓⠀Is Case-Sensitive",
				"category": "Sort JSON"
			}
		],
		"configuration": {
			"title": "Sort JSON",
			"properties": {
				"sort-json.settings.sortType": {
					"order": 1,
					"type": "string",
					"default": "Key",
					"enum": [
						"Key",
						"Key Length",
						"Value",
						"Value Length",
						"Value Type"
					],
					"description": "Set JSON Object Sorting Type"
				},
				"sort-json.settings.isCaseSensitive": {
					"order": 2,
					"type": "boolean",
					"default": false,
					"description": "Set to true to compare with case sensitive in sorting"
				},
				"sort-json.settings.orderOverride": {
					"order": 3,
					"type": "array",
					"default": [],
					"items": {
						"type": "string"
					},
					"additionalItems": true,
					"description": "Settings to override the sort order (i.e. to be at the start of the order)"
				},
				"sort-json.settings.sortValueTypeOrder": {
					"order": 4,
					"type": "array",
					"default": [
						"Boolean",
						"Null",
						"Empty",
						"Number",
						"String",
						"Array",
						"PlainObject"
					],
					"enum": [
						"Boolean",
						"Null",
						"Empty",
						"Number",
						"String",
						"Array",
						"PlainObject"
					],
					"items": {
						"type": "string"
					},
					"uniqueItems": true,
					"additionalItems": false,
					"description": "Set Value Type Sort Order. This order works when you select sortType as 'Value type'."
				},
				"sort-json.settings.ignoreArraysOnDeepSort": {
					"order": 5,
					"type": "boolean",
					"default": false,
					"description": "Set to true to ignore Arrays on deep sort"
				},
				"sort-json.settings.ignoreObjectsOnDeepSort": {
					"order": 6,
					"type": "boolean",
					"default": false,
					"description": "Set to true to ignore Objects on deep sort"
				},
				"sort-json.settings.deepSortLevel": {
					"order": 7,
					"type": "number",
					"default": -1,
					"description": "Set deep sort level. Set to -1 for full deep sort"
				},
				"sort-json.settings.excludePaths": {
					"order": 8,
					"type": "array",
					"default": [],
					"items": {
						"type": "string"
					},
					"additionalItems": true,
					"description": "Set exclude path for deep sort"
				},
				"sort-json.settings.customComparisons": {
					"type": "array",
					"additionalItems": true,
					"default": [],
					"description": "Provide your custom comparison to list in the custom comparisons quick pick",
					"items": {
						"type": "object",
						"additionalProperties": false,
						"required": [
							"comparison",
							"description"
						],
						"examples": [
							{
								"comparison": "a - b",
								"description": "sort ascending"
							}
						],
						"properties": {
							"comparison": {
								"type": "string",
								"examples": [
									"a - b",
									"b - a",
									"a.toLowerCase() - b.toLowerCase()"
								],
								"description": "Set Custom Comparison code"
							},
							"description": {
								"type": "string"
							}
						}
					}
				},
				"sort-json.settings.contextMenu": {
					"order": 9,
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"sortJSONSubMenu": {
							"type": "boolean",
							"description": "Show All sort json commands inside Sort JSON sub menu "
						},
						"sortJSON": {
							"type": "boolean"
						},
						"sortJSONDeep": {
							"type": "boolean"
						},
						"sortJSONReverse": {
							"type": "boolean"
						},
						"sortJSONDeepReverse": {
							"type": "boolean"
						},
						"sortJSONByCustomComparison": {
							"type": "boolean"
						},
						"setSortType": {
							"type": "boolean"
						},
						"caseSensitive": {
							"type": "boolean"
						}
					},
					"default": {
						"sortJSONSubMenu": true,
						"sortJSON": false,
						"sortJSONDeep": false,
						"sortJSONReverse": false,
						"sortJSONDeepReverse": false,
						"sortJSONByCustomComparison": false,
						"setSortType": false,
						"caseSensitive": false
					},
					"description": "Which editor context menu commands to show"
				}
			}
		},
		"menus": {
			"commandPalette": [
				{
					"command": "sort-json.setCaseSensitive",
					"title": "Is Case-Sensitive",
					"category": "Sort JSON"
				},
				{
					"command": "sort-json.setCaseSensitive",
					"when": "false"
				},
				{
					"command": "sort-json.setCaseInSensitive",
					"when": "false"
				}
			],
			"editor/context": [
				{
					"when": "config.sort-json.settings.contextMenu.sortJSON",
					"command": "sort-json.sortJSON",
					"group": "sort-json@1"
				},
				{
					"when": "config.sort-json.settings.contextMenu.sortJSONDeep",
					"command": "sort-json.sortJSONDeep",
					"group": "sort-json@2"
				},
				{
					"when": "config.sort-json.settings.contextMenu.sortJSONReverse",
					"command": "sort-json.sortJSONReverse",
					"group": "sort-json@3"
				},
				{
					"when": "config.sort-json.settings.contextMenu.sortJSONDeepReverse",
					"command": "sort-json.sortJSONDeepReverse",
					"group": "sort-json@4"
				},
				{
					"when": "config.sort-json.settings.contextMenu.sortJSONByCustomComparison",
					"command": "sort-json.sortJSONByCustomComparison",
					"group": "sort-json@5"
				},
				{
					"when": "config.sort-json.settings.contextMenu.sortJSONSubMenu",
					"submenu": "sort-json.subMenu",
					"group": "sort-json"
				}
			],
			"sort-json.subMenu": [
				{
					"command": "sort-json.sortJSON",
					"group": "sort-json@1"
				},
				{
					"command": "sort-json.sortJSONDeep",
					"group": "sort-json@2"
				},
				{
					"command": "sort-json.sortJSONReverse",
					"group": "sort-json@3"
				},
				{
					"command": "sort-json.sortJSONDeepReverse",
					"group": "sort-json@4"
				},
				{
					"command": "sort-json.sortJSONByCustomComparison",
					"group": "sort-json@5"
				},
				{
					"command": "sort-json.setSortType",
					"group": "sort-json@6"
				},
				{
					"when": "!config.sort-json.settings.isCaseSensitive",
					"command": "sort-json.setCaseSensitive",
					"group": "sort-json@7"
				},
				{
					"when": "config.sort-json.settings.isCaseSensitive",
					"command": "sort-json.setCaseInSensitive",
					"group": "sort-json@7"
				}
			]
		},
		"submenus": [
			{
				"id": "sort-json.subMenu",
				"label": "Sort JSON",
				"group": "sort-json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/json-parse-helpfulerror": "^1.0.1",
		"@types/lodash": "^4.14.173",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"json-parse-helpfulerror": "^1.0.3",
		"lodash": "^4.17.21"
	}
}
